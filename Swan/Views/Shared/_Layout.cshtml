<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewBag.Title Swan</title>
  @Styles.Render("~/Content/devoops-master/plugins/bootstrap/bootstrap.css")
  @Styles.Render("~/Content/devoops-master/plugins/jquery-ui/jquery-ui.min.css")
  @Styles.Render("~/Content/devoops-master/css/font-awesome.min.css")
  @Styles.Render("~/Content/bootstrapdatepicker.css")
  @Styles.Render("~/Content/devoops-master/plugins/fancybox/jquery.fancybox.css")
  @Styles.Render("~/Content/devoops-master/plugins/fullcalendar/fullcalendar.css")
  @Styles.Render("~/Content/devoops-master/plugins/xcharts/xcharts.min.css")
  @Styles.Render("~/Content/devoops-master/plugins/select2/select2.css")
  @Styles.Render("~/Content/devoops-master/plugins/justified-gallery/justifiedGallery.css")
  @Styles.Render("~/Content/devoops-master/css/style_v2.css")
  @Styles.Render("~/Content/devoops-master/plugins/chartist/chartist.min.css")
  

  <script src="/Content/devoops-master/plugins/jquery/jquery.min.js"></script>
  <script src="/Content/devoops-master/plugins/jquery-ui/jquery-ui.min.js"></script>
  <!-- Include all compiled plugins (below), or include individual files as needed -->
  <script src="/Content/devoops-master/plugins/bootstrap/bootstrap.min.js"></script>
  <script src="/Content/devoops-master/plugins/justified-gallery/jquery.justifiedGallery.min.js"></script>
  <script src="/Content/devoops-master/plugins/tinymce/tinymce.min.js"></script>
  <script src="/Content/devoops-master/plugins/tinymce/jquery.tinymce.min.js"></script>
  <!-- All functions for this theme + document.ready processing -->
  <script src="/Content/devoops-master/js/devoops.js"></script>
  @RenderSection("scripts", required: false)
</head>
<body>
<!--Start Header-->
<div id="screensaver">
  <canvas id="canvas"></canvas>
  <i class="fa fa-lock" id="screen_unlock"></i>
</div>
<div id="modalbox">
  <div class="devoops-modal">
    <div class="devoops-modal-header">
      <div class="modal-header-name">
        <span>Basic table</span>
      </div>
      <div class="box-icons">
        <a class="close-link">
          <i class="fa fa-times"></i>
        </a>
      </div>
    </div>
    <div class="devoops-modal-inner">
    </div>
    <div class="devoops-modal-bottom">
    </div>
  </div>
</div>
<header class="navbar">
  <div class="container-fluid expanded-panel">
    <div class="row">
      <div id="logo" class="col-xs-12 col-sm-2">
        <a href="/">Swan</a>
      </div>
      <div id="top-panel" class="col-xs-12 col-sm-10">
        <div class="row">
          <div class="col-xs-8 col-sm-4">
            <div id="search">
              <input type="text" placeholder="search"/>
              <i class="fa fa-search"></i>
            </div>
          </div>
          <div class="col-xs-4 col-sm-8 top-panel-right">
            <a href="#" class="about">about</a>
            <a href="index_v1.html" class="style1"></a>
            <ul class="nav navbar-nav pull-right panel-menu">
              <li class="hidden-xs">
                <a href="index.html" class="modal-link">
                  <i class="fa fa-bell"></i>
                  <span class="badge">7</span>
                </a>
              </li>
              <li class="hidden-xs">
                <a class="ajax-link" href="/Content/devoops-master/ajax/calendar.html">
                  <i class="fa fa-calendar"></i>
                  <span class="badge">7</span>
                </a>
              </li>
              <li class="hidden-xs">
                <a href="/Content/devoops-master/ajax/page_messages.html" class="ajax-link">
                  <i class="fa fa-envelope"></i>
                  <span class="badge">7</span>
                </a>
              </li>
              <li class="dropdown">
                <a href="#" class="dropdown-toggle account" data-toggle="dropdown">
                  <div class="avatar">
                    <img src="/Content/devoops-master/img/avatar.jpg" class="img-circle" alt="avatar"/>
                  </div>
                  <i class="fa fa-angle-down pull-right"></i>
                  <div class="user-mini pull-right">
                    <span class="welcome text-center">Welcome,</span>
                    <span>@HttpContext.Current.User.Identity.Name</span>
                  </div>
                </a>
                <ul class="dropdown-menu">
                  <li>
                    <a href="#">
                      <i class="fa fa-user"></i>
                      <span>Profile</span>
                    </a>
                  </li>
                  <li>
                    <a href="/Content/devoops-master/ajax/page_messages.html" class="ajax-link">
                      <i class="fa fa-envelope"></i>
                      <span>Messages</span>
                    </a>
                  </li>
                  <li>
                    <a href="/Content/devoops-master/ajax/gallery_simple.html" class="ajax-link">
                      <i class="fa fa-picture-o"></i>
                      <span>Albums</span>
                    </a>
                  </li>
                  <li>
                    <a href="/Content/devoops-master/ajax/calendar.html" class="ajax-link">
                      <i class="fa fa-tasks"></i>
                      <span>Tasks</span>
                    </a>
                  </li>
                  <li>
                    <a href="#">
                      <i class="fa fa-cog"></i>
                      <span>Settings</span>
                    </a>
                  </li>
                  <li>
                    <a href="/Account/LogOff/">
                      <i class="fa fa-power-off"></i>
                      <span>Logout</span>
                    </a>
                  </li>
                </ul>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
<!--End Header-->
<!--Start Container-->
<div id="main" class="container-fluid">
  <div class="row">
    @Html.Action("Navigation", "Home", new {category = ViewContext.RouteData.Values["controller"].ToString()})
    <div id="content" class="col-xs-12 col-sm-10">
      <div id="about">
        <div class="about-inner">
          <h4 class="page-header">Open-source admin theme for you</h4>
          <p>DevOOPS team</p>
          <p>Homepage - <a href="http://devoops.me" target="_blank">http://devoops.me</a></p>
          <p>Email - <a href="mailto:devoopsme@gmail.com">devoopsme@gmail.com</a></p>
          <p>Twitter - <a href="http://twitter.com/devoopsme" target="_blank">http://twitter.com/devoopsme</a></p>
          <p>Donate - BTC 123Ci1ZFK5V7gyLsyVU36yPNWSB5TDqKn3</p>
        </div>
      </div>
      <div id="ajax-content">@RenderBody()</div>
    </div>
      <!--End Content-->
    </div>
</div>
</body>

</html>
